@article{ ISI:000377738500007,
Author = {Quinet, Muriel and Warzee, Martin and Vanderplanck, Maryse and Michez,
   Denis and Lognay, Georges and Jacquemart, Anne-Laure},
Title = {{Do floral resources influence pollination rates and subsequent fruit set
   in pear (Pyrus communis L.) and apple (Malus x domestica Borkh)
   cultivars?}},
Journal = {{EUROPEAN JOURNAL OF AGRONOMY}},
Year = {{2016}},
Volume = {{77}},
Number = {{SI}},
Pages = {{59-69}},
Month = {{JUL}},
Abstract = {{Pear and apple are among the main fruit crops worldwide. These species
   can be planted in mixed orchards, and they both depend on insect
   pollination for fruit set. As pollinating insects are attracted by the
   floral resources, we investigated nectar and pollen production and
   chemical composition in four pear ('Concorde', `Conference', `Doyenne du
   Cornice', `Triomphe de Vienne') and five apple ('Braeburn', `Golden
   Reinders', lonagored', `Pinova', `Wellant') cultivars commonly grown in
   Belgium. We also investigated whether insect flower visitation rate and
   pollination efficiency are linked to floral resource quantity and
   quality. The pear cultivars flowered one week before the apple cultivars
   in early spring, and their flowers were about six times less visited by
   insects. The visitors foraged more on the pollen of the pear trees and
   the nectar of the apple trees. Pear flowers produced higher volumes of
   nectar than apple flowers (1.3-3.2 mu l vs. 0.4-0.6 mu l), but with
   lower sugar concentration (9.6\%-10.8\% vs. 28.3\%-36.4\%). Pear flowers
   also produced fewer pollen grains per anther than apple flowers
   (2425-4937 vs. 3284-7919), but these had higher polypeptide (346-362 mu
   g/mg vs. 216-303 mu g/mg), amino-acid (40-77 mu g/mg vs. 12-18 mu g/mg)
   and phytosterol (21-47 mu g/mg vs. 15-43 mu g/mg) concentrations. The
   foraging behavior of the insects is thus better explained by nectar and
   pollen quality rather than quantity. Despite the differences in flower
   visitation rates, pollination of both species resulted in valuable fruit
   production. (C) 2016 Elsevier B.V. All rights reserved.}},
