@article{ ISI:000400557900024,
Author = {de Oliveira Lima, Felipe Varussa and Silvestre, Rogerio},
Title = {{Checklist of bees (Hymenoptera, Apidae sensu lato) from Mato Grosso do
   Sul state, Brazil}},
Journal = {{IHERINGIA SERIE ZOOLOGIA}},
Year = {{2017}},
Volume = {{107}},
Number = {{S}},
Abstract = {{In this paper a current list of the bee species from Mato Grosso do Sul
   State, Brazil is presented and available in the online database of
   collections and reported in the literature, besides own collections held
   in different localities in the state, encompassing different vegetation
   types. The total number of bee species recorded for Mato Grosso do Sul,
   including morphospecies, is 386 species, distributed in 107 genera and
   five subfamilies. The most diverse subfamily is Apinae, followed by
   Megachilinae, Halictinae, Colletinae and Andreninae. We added new
   distribution records of 97 species of bees to Mato Grosso do Sul.}},
