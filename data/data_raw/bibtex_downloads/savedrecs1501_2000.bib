@article{ ISI:000413202900004,
Author = {Prokop, Jakub and Dehon, Manuel and Michez, Denis and Engel, Michael S.},
Title = {{An Early Miocene bumble bee from northern Bohemia (Hymenoptera, Apidae)}},
Journal = {{ZOOKEYS}},
Year = {{2017}},
Number = {{710}},
Pages = {{43-63}},
Abstract = {{A new species of fossil bumble bee (Apinae: Bombini) is described and
   figured from Early Miocene (Burdigalian) deposits of the Most Basin at
   the Bilina Mine, Czech Republic. Bombus trophonius sp. n., is placed
   within the subgenus Cullumanobombus Vogt and distinguished from the
   several species groups therein. The species is apparently most similar
   to the Nearctic B. (Cullumanobombus) rufocinctus Cresson, the
   earliest-diverging species within the clade and the two may be related
   only by symplesiomorphies. The age of the fossil is in rough accordance
   with divergence estimations for Cullumanobombus.}},
