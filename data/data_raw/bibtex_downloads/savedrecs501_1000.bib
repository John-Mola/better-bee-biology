@article{ ISI:000449247600064,
Author = {Boff, Samuel and Friedel, Anna and Mussury, Rosilda Mara and Lenis,
   Patricia Roseti and Raizer, Josue},
Title = {{Changes in social behavior are induced by pesticide ingestion in a
   Neotropical stingless bee}},
Journal = {{ECOTOXICOLOGY AND ENVIRONMENTAL SAFETY}},
Year = {{2018}},
Volume = {{164}},
Pages = {{548-553}},
Month = {{NOV 30}},
Abstract = {{Throughout evolutionary history bees have developed complex
   communication systems. For social bees, communication is important for
   both the individual and the development of the colony. Successful
   communication helps bees to recognize relatives, defend the colony, and
   promote recruitment to optimize foraging of floral-resources. Bees'
   contribution to pollination is of broad environmental and economic
   importance. However, studies have reported that anthropogenic actions,
   such as the use of pesticides, negatively affect bee survival and
   behavior. We tested the effect of a commercially available pesticide mix
   containing two pesticide classes, a neonicotinoid and a pyrethroid, on
   the social behavior of the stingless bee, Melipona quadrifasciata
   (Lepeletier, 1863). After determining a sublethal dose of the
   pesticides, we tested the effect of an acute dose on antennation and
   trophallaxis behaviors of worker bees. Our results showed a drastic
   reduction in the communication and social interactions of bees.}},
