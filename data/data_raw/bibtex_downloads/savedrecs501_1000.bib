@article{ ISI:000456140700036,
Author = {Grab, Heather and Branstetter, Michael G. and Amon, Nolan and
   Urban-Mead, Katherine R. and Park, Mia G. and Gibbs, Jason and Blitzer,
   Eleanor J. and Poveda, Katja and Loeb, Greg and Danforth, Bryan N.},
Title = {{Agriculturally dominated landscapes reduce bee phylogenetic diversity
   and pollination services}},
Journal = {{SCIENCE}},
Year = {{2019}},
Volume = {{363}},
Number = {{6424}},
Pages = {{282+}},
Month = {{JAN 18}},
Abstract = {{Land-use change threatens global biodiversity and may reshape the tree
   of life by favoring some lineages over others. Whether phylogenetic
   diversity loss compromises ecosystem service delivery remains unknown.
   We address this knowledge gap using extensive genomic, community, and
   crop datasets to examine relationships among land use, pollinator
   phylogenetic structure, and crop production. Pollinator communities in
   highly agricultural landscapes contain 230 million fewer years of
   evolutionary history; this loss was strongly associated with reduced
   crop yield and quality. Our study links landscape-mediated changes in
   the phylogenetic structure of natural communities to the disruption of
   ecosystem services. Measuring conservation success by species counts
   alone may fail to protect ecosystem functions and the full diversity of
   life from which they are derived.}},
