@article{ ISI:000429509300014,
Author = {Kozuharova, Ekaterina K.},
Title = {{Functional flower morphology and entomophilous pollination syndromes in
   Cape Kaliakra Nature Reserve (North Black Sea coast, Bulgaria)}},
Journal = {{ACTA ZOOLOGICA BULGARICA}},
Year = {{2018}},
Number = {{11}},
Pages = {{87-90}},
