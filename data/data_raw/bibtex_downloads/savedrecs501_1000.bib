@article{ ISI:000441212000024,
Author = {Ghosh, Sampat and Jung, Chuleui},
Title = {{Contribution of insect pollination to nutritional security of minerals
   and vitamins in Korea}},
Journal = {{JOURNAL OF ASIA-PACIFIC ENTOMOLOGY}},
Year = {{2018}},
Volume = {{21}},
Number = {{2}},
Pages = {{598-602}},
Month = {{JUN}},
Abstract = {{Crop production for human food and nutritional security is largely
   dependent on pollination process especially by insects. We assessed the
   role of pollination in connection with micronutrient supply from edible
   crops to the Korean population. The study included 42 predominant crop
   types of varying pollinator dependence. A higher pollinator dependency
   than present global average was noted in connection with all minerals
   deemed nutritionally important and vitamins. Pollinator-dependent (PD)
   crops provide a substantial share of micro nutrients to the population,
   especially with regard to vitamins C and E and elements iron. A positive
   trend was found in the PD supply of all micronutrients over the last 30
   years in Korea, but the rate of increment was somewhat lower for
   vitamins C and K as well as folate. The results demonstrated the
   importance of pollination for human nutritional security and
   conservation of insect pollination system for the sustained ecosystem
   service.}},
