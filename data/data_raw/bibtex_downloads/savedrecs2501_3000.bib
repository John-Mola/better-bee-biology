@article{ ISI:000342900900009,
Author = {Kubo, Ryohei and Ono, Masato},
Title = {{Innate attractant for long-tongued bumblebee, Bombus diversus diversus
   in floral scent of bumblebee-pollinated epiparasitic plant,
   Monotropastrum humile (Ericaceae)}},
