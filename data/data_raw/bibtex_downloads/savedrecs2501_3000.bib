@article{ ISI:000332776600010,
Author = {Sarzetti, Laura C. and Dinghi, Pablo A. and Genise, Jorge F. and
   Bedatou, Emilio and Verde, Mariano},
Title = {{CURVED FOSSIL BEE CELLS AS TOOLS FOR RECONSTRUCTING THE EVOLUTIONARY
   HISTORY AND PALAEOGEOGRAPHICAL DISTRIBUTION OF DIPHAGLOSSINAE ( APOIDEA,
   COLLETIDAE)}},
Journal = {{PALAEONTOLOGY}},
Year = {{2014}},
Volume = {{57}},
Number = {{2}},
Pages = {{447-455}},
Month = {{MAR}},
Abstract = {{The new ichnospecies Celliforma curvata is described to include curved
   fossil bee cells from Argentina, Uruguay and the USA. The upper part of
   the cell (neck) of the new ichnospecies is curved, and accordingly, it
   can be attributed to bees of the subfamily Diphaglossinae (Colletidae).
   The oldest record of C.curvata, from the early Eocene of North America
   (52-49Ma), provides a minimum age for the appearance of this subfamily,
   in accordance with an already proposed calibrated phylogeny. It is also
   proposed that these fossil cells could be used for future calibrations
   of molecular clocks. C.curvata indicates that Diphaglossinae had a
   widespread distribution, from southern Utah to extra-Andean Patagonia at
   42 degrees S. In contrast, extant representatives reach only 38 degrees
   S in this region. Diphaglossinae were more extended southwards in the
   past thanks to better environmental conditions in extra-Andean
   Patagonia.}},
