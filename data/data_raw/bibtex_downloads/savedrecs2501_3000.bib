@article{ ISI:000345556200017,
Author = {Quaranta, Marino and Sommaruga, Angelo and Balzarini, Patrizia and
   Felicioli, Antonio},
Title = {{A new species for the bee fauna of Italy: Megachile sculpturalis
   continues its colonization of Europe}},
Journal = {{BULLETIN OF INSECTOLOGY}},
Year = {{2014}},
Volume = {{67}},
Number = {{2}},
Pages = {{287-293}},
Month = {{DEC}},
Abstract = {{During studies of bee diversity and nesting biology in the province of
   Verbania, Piedmont, northern Italy, a bee species recognized as foreign
   to the Italian fauna was observed. Observations were conducted on their
   life cycle and nesting behaviour, and specimens were collected for
   subsequent identification. The species in question was determined to be
   Megachile (Callomegachile) sculpturalis Smith (Hymenoptera
   Megachilidae), a species native to East Asia. The population has been
   present in the area since 2009 and the number of nests detected is
   expanding. The flight activity is from approximately 10 July to 15
   August. We examine hypotheses that may explain the arrival of the
   insect. In addition, pollen samples were taken from the nests,
   preliminary results of pollen analysis are provided, and the potential
   impact of this invasive species on the local fauna and flora is
   examined.}},
