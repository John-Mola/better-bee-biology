@article{ ISI:000414938000002,
Author = {Ogilvie, Jane E. and Griffin, Sean R. and Gezon, Zachariah J. and
   Inouye, Brian D. and Underwood, Nora and Inouye, David W. and Irwin,
   Rebecca E.},
Title = {{Interannual bumble bee abundance is driven by indirect climate effects
   on floral resource phenology}},
Journal = {{ECOLOGY LETTERS}},
Year = {{2017}},
Volume = {{20}},
Number = {{12}},
Pages = {{1507-1515}},
Month = {{DEC}},
Abstract = {{Climate change can influence consumer populations both directly, by
   affecting survival and reproduction, and indirectly, by altering
   resources. However, little is known about the relative importance of
   direct and indirect effects, particularly for species important to
   ecosystem functioning, like pollinators. We used structural equation
   modelling to test the importance of direct and indirect (via floral
   resources) climate effects on the interannual abundance of three
   subalpine bumble bee species. In addition, we used long-term data to
   examine how climate and floral resources have changed over time. Over 8
   years, bee abundances were driven primarily by the indirect effects of
   climate on the temporal distribution of floral resources. Over 43 years,
   aspects of floral phenology changed in ways that indicate
   species-specific effects on bees. Our study suggests that climate-driven
   alterations in floral resource phenology can play a critical role in
   governing bee population responses to global change.}},
