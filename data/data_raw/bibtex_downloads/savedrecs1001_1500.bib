@article{ ISI:000405534500020,
Author = {Arce, Andres N. and David, Thomas I. and Randall, Emma L. and Rodrigues,
   Ana Ramos and Colgan, Thomas J. and Wurm, Yannick and Gill, Richard J.},
Title = {{Impact of controlled neonicotinoid exposure on bumblebees in a realistic
   field setting}},
