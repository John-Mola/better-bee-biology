@article{ ISI:000360632600002,
Author = {Cutler, G. Christopher and Rix, Rachel R.},
Title = {{Can poisons stimulate bees? Appreciating the potential of hormesis in
   bee-pesticide research}},
Journal = {{PEST MANAGEMENT SCIENCE}},
Year = {{2015}},
Volume = {{71}},
Number = {{10}},
Pages = {{1368-1370}},
Month = {{OCT}},
Abstract = {{Hormesis, a biphasic dose response whereby exposure to low doses of a
   stressor can stimulate biological processes, has been reported in many
   organisms, including pest insects when they are exposed to low doses of
   a pesticide. However, awareness of the hormesis phenomenon seems to be
   limited among bee researchers, in spite of the increased emphasis of
   late on pollinator toxicology and risk assessment. In this commentary,
   we show that there are several examples in the literature of substances
   that are toxic to bees at high doses but stimulatory at low doses.
   Appreciation of the hormetic dose response by bee researchers will
   improve our fundamental understanding of how bees respond to low doses
   of chemical stressors, and may be useful in pollinator risk assessment.
   (c) 2015 Society of Chemical Industry}},
