@article{ ISI:000351223600010,
Author = {Pokorny, Tamara and Loose, Dirk and Dyker, Gerald and Quezada-Euan, J.
   Javier G. and Eltz, Thomas},
Title = {{Dispersal ability of male orchid bees and direct evidence for long-range
   flights}},
