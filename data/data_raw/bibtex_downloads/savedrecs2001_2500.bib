@article{ ISI:000354131800001,
Author = {Yocum, George D. and Rinehart, Joseph P. and Horvath, David P. and Kemp,
   William P. and Bosch, Jordi and Alroobi, Rami and Salem, Saeed},
Title = {{Key molecular processes of the diapause to post-diapause quiescence
   transition in the alfalfa leafcutting bee Megachile rotundata identified
   by comparative transcriptome analysis}},
Journal = {{PHYSIOLOGICAL ENTOMOLOGY}},
Year = {{2015}},
Volume = {{40}},
Number = {{2}},
Pages = {{103-112}},
Month = {{JUN}},
Abstract = {{Insect diapause (dormancy) synchronizes an insect's life cycle to
   seasonal changes in the abiotic and biotic resources required for
   development and reproduction. Transcription analysis of diapause to
   post-diapause quiescent transition in the alfalfa leafcutting bee
   Megachile rotundataFabricius identifies 643 post-diapause up-regulated
   gene transcripts and 242 post-diapause down-regulated transcripts. The
   log(2) fold change in gene expression levels ranges from -5 to 7.
   Transcripts from several pivotal diapause-related processes, including
   chromatin remodelling, cellular signalling pathways, microRNA
   processing, anaerobic glycolysis, cell cycle arrest and neuroendocrine
   control, are identified as being differentially expressed during the
   diapause to post-diapause transition. In conjunction with studies from
   other insect species, the data indicate that there are several common
   mechanisms of diapause control and maintenance.}},
