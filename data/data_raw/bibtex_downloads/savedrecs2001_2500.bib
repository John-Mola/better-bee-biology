Address = {{1160 BATTERY STREET, STE 100, SAN FRANCISCO, CA 94111 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Dotterl, S (Reprint Author), Salzburg Univ, Dept Organism Biol, A-5020 Salzburg, Austria.
   Milet-Pinheiro, Paulo; Ayasse, Manfred, Univ Ulm, Inst Expt Ecol, D-89069 Ulm, Germany.
   Doetterl, Stefan, Univ Bayreuth, Dept Plant Systemat, Bayreuth, Germany.}},
DOI = {{10.1371/journal.pone.0128577}},
