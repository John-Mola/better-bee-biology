@article{ ISI:000349452500009,
Author = {Adderley, Lorraine J. and Vamosi, Jana C.},
Title = {{SPECIES AND PHYLOGENETIC HETEROGENEITY IN VISITATION AFFECTS
   REPRODUCTIVE SUCCESS IN AN ISLAND SYSTEM}},
Journal = {{INTERNATIONAL JOURNAL OF PLANT SCIENCES}},
Year = {{2015}},
Volume = {{176}},
Number = {{2}},
Pages = {{186-196}},
Month = {{FEB}},
Abstract = {{Plant-pollinator mutualisms can be disrupted in fragmented and isolated
   populations. Isolated populations attract fewer and less diverse
   pollinators, but it is not generally known whether this is due to
   reductions in the abundance and/or diversity of flowers at a site or to
   the biogeography of pollinators (or both). Islands represent one system
   potentially suitable to analyze the effects of spatial isolation. Here,
   we examine the contribution that differences in visitor composition make
   to increased selfing and seed production by examining the major visitors
   to Plectritis congesta in populations on the Gulf Islands and Vancouver
   Island, We find that (1) connectivity is more strongly associated with
   visitor assemblages and seed production than components of the floral
   community and (2) taking phylogeny into account suggests that increased
   visitation by solitary bees as opposed to other functional groups
   increases female fitness in P. congesta.}},
