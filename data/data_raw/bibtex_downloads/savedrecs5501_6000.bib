@article{ ISI:000077431800001,
Author = {Forfang, AS and Olesen, JM},
Title = {{Male-biased sex ratio and promiscuous pollination in the dioecious
   island tree Laurus azorica (Lauraceae)}},
Journal = {{PLANT SYSTEMATICS AND EVOLUTION}},
Year = {{1998}},
Volume = {{212}},
Number = {{3-4}},
Pages = {{143-157}},
Abstract = {{Pollination of Laurus azorica (Lauraceae), a dioecious Macaronesian
   tree, was studied. Male and female trees had the same size distribution.
   The population had 2.5 times as many male trees as females. In addition,
   males produced more flowers, and their inflorescences lasted longer.
   Individual flower lifetime and length of flowering season were the same
   in both sexes. Between the years of observation, one tree changed sex.
   Pollinators were Halictinae bees and the fly Tachina canariensis. The
   bees collected pollen and nectar and the fly collected nectar from both
   sexes. Both species visited other plants as well. The evolution of
   breeding systems in Lauraceae is discussed.}},
DOI = {{10.1007/BF01089736}},
ISSN = {{0378-2697}},
EISSN = {{2199-6881}},
ResearcherID-Numbers = {{Olesen, Jens M/A-3011-2009}},
