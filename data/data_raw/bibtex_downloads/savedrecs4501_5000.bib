@article{ ISI:000260960900001,
Author = {Valterova, Irena and Kunze, Jan and Gumbert, Andreas and Luxova, Anna
   and Liblikas, Ilme and Kalinova, Blanka and Borg-Karlson, Anna-Karin},
Title = {{Male bumble bee pheromonal components in the scent of deceit pollinated
   orchids; unrecognized pollinator cues?}},
Journal = {{ARTHROPOD-PLANT INTERACTIONS}},
