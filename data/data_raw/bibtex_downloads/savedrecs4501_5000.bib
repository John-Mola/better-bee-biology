DOI = {{10.1890/06-1455}},
ISSN = {{0012-9658}},
Keywords = {{alpine skypilot; Bombus kirbyellus; bumble bee pollinators; cheaters;
   density dependence; exploitation; flower size; Formica neorufibarbus
   gelida; mutualism; nectar-thieving ants; Polemonium viscosum; selection}},
