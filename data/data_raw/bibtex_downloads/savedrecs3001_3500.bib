Address = {{1160 BATTERY STREET, STE 100, SAN FRANCISCO, CA 94111 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Zhang, WH (Reprint Author), Harvard Univ Herbaria, Dept Organism \& Evolutionary Biol, Cambridge, MA 02138 USA.
   Zhang, Wenheng; Kramer, Elena M.; Davis, Charles C., Harvard Univ Herbaria, Dept Organism \& Evolutionary Biol, Cambridge, MA 02138 USA.}},
DOI = {{10.1371/journal.pone.0036033}},
Article-Number = {{e36033}},
