@article{ ISI:000313784500009,
Author = {Sedivy, Claudio and Dorn, Silvia and Mueller, Andreas},
Title = {{Evolution of nesting behaviour and kleptoparasitism in a selected group
   of osmiine bees (Hymenoptera: Megachilidae)}},
Journal = {{BIOLOGICAL JOURNAL OF THE LINNEAN SOCIETY}},
Year = {{2013}},
Volume = {{108}},
Number = {{2}},
Pages = {{349-360}},
Month = {{FEB}},
Abstract = {{The construction of nests to rear offspring is restricted to vertebrates
   and few insect taxa, such as termites, wasps, and bees. Among bees,
   species of the family Megachilidae are characterized by a particularly
   high diversity in nest construction behaviour. Many megachilid bees nest
   in excavated burrows in the ground, others place their brood cells in a
   variety of above-ground cavities or attach them to the surface of a
   substrate, and yet others have adopted a kleptoparasitic habit.
   Evolutionary transitions between the different nesting sites and between
   conventional nesting and kleptoparasitism in bees are poorly understood.
   In the present study, we traced the evolution of nesting site selection
   and kleptoparasitism in the AnnosmiaHoplitis group (Osmiini), which
   displays an exceptionally high diversity in nesting behaviour. We found
   that the evolution of nesting behaviour proceeded unidirectionally from
   nesting in excavated burrows in the ground to nesting in rock
   depressions and cavities, followed by the colonization of snail shells
   and insect borings in dead wood or hollow stems. Kleptoparasitism
