@article{ ISI:000258594600033,
Author = {Sreekala, A. K. and Pandurangan, A. G. and Ramasubbu, R. and Kulloli, S.
   K.},
Title = {{Reproductive biology of Impatiens coelotropis Fischer, a critically
   endangered balsam from the Southern Western Ghats}},
Journal = {{CURRENT SCIENCE}},
Year = {{2008}},
Volume = {{95}},
Number = {{3}},
Pages = {{386-388}},
Month = {{AUG 10}},
Abstract = {{A study of floral phenology, pollination biology and breeding system of
   Impatiens coelotropis Fischer, an endemic and endangered balsam of
   Western Ghats was carried out in wild conditions. The flowers are
   beautifully coloured and open in the night between 2130 and 2330 h.
   Anther dehisced one day before flower opening, which confirmed the
   protandrous condition. Flowers offer pollen and nectar to the visitors,
   which include predominantly honey bees, hawk moths, butterflies, etc. It
   reproduces by means of cross-pollination. Manual pollinations revealed
   that the species permits only xenogamous pollination. The percentage of
   fruit set in manually pollinated flowers is higher than that resulting
   from open pollination. Its high altitude habitat, narrow environmental
   niche, fragmentation of population, poor fruit set, low percentage of
   seed germination and floral damage caused by the insects could be the
   reasons for its limited distribution in the wild.}},
