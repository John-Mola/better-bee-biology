@article{ ISI:000272299700010,
Author = {Matsuura, Shohei and Matsushita, Yosuke and Kozuka, Reiko and Shimizu,
   Sachiko and Tsuda, Shinya},
Title = {{Transmission of Tomato chlorotic dwarf viroid by bumblebees (Bombus
   ignitus) in tomato plants}},
Journal = {{EUROPEAN JOURNAL OF PLANT PATHOLOGY}},
Year = {{2010}},
Volume = {{126}},
Number = {{1}},
Pages = {{111-115}},
Month = {{JAN}},
Abstract = {{Quantitative PCR revealed that Tomato chlorotic dwarf viroid (TCDVd) was
   present in substantial amounts in viroid-infected tomato flowers.
   Healthy tomato plants were arranged in two different glasshouses, and
   plants were mechanically inoculated with TCDVd. Bumblebees (Bombus
   ignitus) were then introduced into the glasshouses to reveal whether the
   viroid was transmitted from infected source plants to neighbouring
   healthy plants. TCDVd infection was found in neighbouring tomato plants
   more than 1 month after the introduction of the bees, some of which
   expressed symptoms, in both glasshouses. Thus, bumblebees transmitted
   TCDVd from tomato to tomato by pollination activities.}},
